<GuiXml>
    <Controls>
        <TopLevelControl name="LibSetsAskBeforeReloadUIDialogXML" inherits="ZO_CustomDialogBase">
            <Dimensions x="500" y="125" />
            <Anchor point="CENTER" offsetY="-45" />
            <Controls>
                <Control name="$(parent)Content" inherits="ZO_DialogContentAnchors">
                    <Controls>
                        <Label name="$(parent)Text" font="ZoFontGame" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_SELECTED" horizontalAlignment="LEFT" text="xxx">
                            <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT"/>
                        </Label>
                    </Controls>
                </Control>
                <Control name="$(parent)Cancel" inherits="ZO_CustomDialogButton" />
                <Control name="$(parent)Accept" inherits="ZO_CustomDialogButton" />
            </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>
